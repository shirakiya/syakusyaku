{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "extends": ["eslint:recommended", "plugin:vue/recommended", "prettier"],
  "ignorePatterns": ["node_modules/"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["vue"],
  "globals": {
    "__dirname": "readable",
    "process": "readable"
  },
  "rules": {}
}
